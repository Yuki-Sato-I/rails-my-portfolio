<div id="tabBlog" class="mytab">
  <h2>Blogs(<%= @blogs.count %> blogs)</h2>
  <div class="productions">
    <% @blogs.each do |blog| %>
      <%= link_to "/blog/#{blog.id}", data: {"turbolinks" => false} do %>
        <div class="my_production" style="height:250px;">
          <div class="blog-contents">
            <p class="title-name"><%= blog.title %></p>
          </div>
          <p class="blog-created">作成日:<%= blog.created_at.strftime("%Y-%m-%d %H:%M") %></p>
        </div>
      <% end %>
    <% end %>
  </div>
</div>
